    {
      "id": "TC-001",
      "description": "Valid login with correct credentials",
      "priority": "High",
      "steps": [
        "Navigate to login page",
        "Enter valid username",
        "Enter valid password",
        "Click login button"
      ],
      "expectedResult": "User is logged in and redirected to dashboard",
      "status": "Not Executed"
    },
    {
      "id": "TC-002",
      "description": "Login with invalid password",
      "priority": "Medium",
      "steps": [
        "Navigate to login page",
        "Enter valid username",
        "Enter invalid password",
        "Click login button"
      ],
      "expectedResult": "Error message 'Invalid credentials' is displayed",
      "status": "Not Executed"
    },
    {
      "id": "TC-003",
      "description": "Login with empty username",
      "priority": "Low",
      "steps": [
        "Navigate to login page",
        "Leave username field empty",
        "Enter valid password",
        "Click login button"
      ],
      "expectedResult": "Field validation error for username appears",
      "status": "Not Executed"
    }
  ],
